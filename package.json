{
  "name": "vj-boucherie",
  "version": "1.0.0",
  "private": true,
  "main": "main.js",
  "build": {
    "appId": "com.benoitmartel.boucherie",
    "extraFiles": [
      {
        "from": "./static/extraResources/",
        "to": "."
      }
    ],
    "win": {
      "target": {
        "target": "NSIS"
      },
      "icon": "static/icon.png"
    },
    "nsis": {
      "include": "build/installer.nsh"
    },
    "directories": {
      "output": "app"
    }
  },
  "scripts": {
    "dev": "nuxt",
    "start": "nuxt start",
    "generate": "nuxt generate && scp -r dist/* benoitma@50.87.233.33:public_html/dev/vj",
    "lint:prettier": "prettier --check .",
    "lint": "yarn lint:prettier",
    "lintfix": "prettier --write --list-different .",
    "electron": "cross-env NODE_ENV=dev electron .",
    "e:build": "cross-env NODE_ENV=production nuxt build && electron-builder --win"
  },
  "dependencies": {
    "body-parser": "^1.20.1",
    "core-js": "^3.19.3",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "howler": "^2.2.3",
    "nuxt": "^2.15.8",
    "vue": "^2.6.14",
    "vue-server-renderer": "^2.6.14",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^4.46.0"
  },
  "devDependencies": {
    "@nuxtjs/style-resources": "^1.2.1",
    "cross-env": "^5.1.4",
    "electron": "^13.1.2",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^2.2.3",
    "prettier": "^2.5.1",
    "sass": "^1.62.0",
    "sass-loader": "^10"
  }
}
